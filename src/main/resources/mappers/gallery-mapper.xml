<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Gallery">
    <insert id="insert">
        insert into gallery_cards values(GALLERY_CARDS_SEQ.NEXTVAL, ${category_type}, ${genre_type1}, #{ai}, #{title}, ${writer}, #{catchphrase}, #{synopsis}, #{thumbnail_url}, #{yn}, default, ${genre_type2})
    </insert>
    <select id="selectAll" resultType="kh.final_project.dto.GalleryView">
        select * from view_gallery_cards
    </select>
</mapper>