<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Member">

    <select id="login" resultType="kh.final_project.dto.MemberDTO">
        select * from members where email = #{email} and password=#{password } and email_type = 1002
    </select>


</mapper>