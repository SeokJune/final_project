<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Email">
        <insert id="insert">
            insert into emailcheck values (${member_type},#{email},${email_type},default)
        </insert>

        <select id="select" resultType="boolean">
            select count(*) from emailcheck where member_type=${member_type} and email = #{email} and email_type=${email_type}
        </select>
</mapper>



